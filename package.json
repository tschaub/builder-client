{
  "name": "openlayers-builder-client",
  "version": "0.0.0",
  "description": "Client for the OpenLayers build service",
  "main": "app/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "test": "eslint server.js lib",
    "build": "webpack --config webpack.production.js index.js",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openlayers/builder-client.git"
  },
  "bugs": {
    "url": "https://github.com/openlayers/builder-client/issues"
  },
  "homepage": "http://github.com/openlayers/builder-client",
  "dependencies": {
    "babel-polyfill": "^6.0.14",
    "classnames": "^2.1.5",
    "isomorphic-fetch": "^2.1.1",
    "openlayers-builder-util": "^0.4.0",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-redux": "^4.0.0",
    "react-select": "^0.9.1",
    "redux": "^3.0.2",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.15",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "css-loader": "^0.23.0",
    "eslint": "^2.3.0",
    "eslint-config-openlayers": "^4.1.0",
    "eslint-plugin-react": "^4.2.1",
    "inline-environment-variables-webpack-plugin": "0.0.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "node-libs-browser": "^1.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^1.12.0"
  },
  "eslintConfig": {
    "extends": "openlayers",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-boolean-value": 2,
      "react/jsx-no-undef": 2,
      "react/jsx-sort-props": 2,
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/no-did-mount-set-state": 2,
      "react/no-did-update-set-state": 2,
      "react/no-multi-comp": 2,
      "react/no-unknown-property": 2,
      "react/prop-types": 2,
      "react/react-in-jsx-scope": 2,
      "react/self-closing-comp": 2,
      "react/sort-prop-types": 2,
      "react/wrap-multilines": 2
    }
  }
}
